<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd
       ">

    <import resource="dubbo-gateway-direct.xml" />

    <!-- for provider -->
    <dubbo:registry address="zookeeper://localhost:2181" />

    <bean class="org.dracula.test.dubbo.gateway.gateway.FakeImpl" id="test-interface-generic" factory-method="getObject">
        <constructor-arg name="objectType" value="org.dracula.test.dubbo.gateway.TestInterface" />
        <constructor-arg name="ref" ref="test-interface-remote" />
    </bean>

    <dubbo:service interface="org.dracula.test.dubbo.gateway.TestInterface" ref="test-interface-generic" />

</beans>